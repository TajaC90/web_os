<!DOCTYPE html>
<html>
<head>
  <title>WebOs</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<header> </header>

<div class="container">
  <div class="frame">

    <div class="col1">
      <div class="bit-10">
          <div class="box">1</div>
      </div>

      <div class="bit-10">
          <div class="box">2</div>
      </div>

      <div class="bit-10">
          <div class="box">3</div>
      </div>

      <div class="bit-10">
          <div class="box">4</div>
      </div>

      <div class="bit-10">
          <div class="box">5</div>
      </div>

      <div class="bit-10">
          <div class="box">6</div>
      </div>
  </div>

  <div class="col2">
      <div class="bit-10">
          <div class="box">
              <img src="icon.png">
          </div>
      </div>

      <div class="bit-10">
          <div class="box">7</div>
      </div>

      <div class="bit-10">
          <div class="box">8</div>
      </div>

      <div class="bit-10">
          <div class="box">9</div>
      </div>

      <div class="bit-10">
          <div class="box">10</div>
      </div>
  </div>

				
	<%= yield %> <!-- !!! -->


  </div> <!-- frame -->
</div> <!-- container -->

	<footer> 
		<img src="icon.png" style="max-width:40px; padding-top:5px;">
	</footer>

</body>
</html>